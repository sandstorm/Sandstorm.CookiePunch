prototype(Sandstorm.CookiePunch:Js.Config) < prototype(Neos.Fusion:Component) {
    cookiePunchConfig = null

    @propTypes {
        cookiePunchConfig = ${PropTypes.object().isRequired()}
    }

    renderer = ${Json.stringify(props.cookiePunchConfig)}
    renderer.@process.windowAssignment = afx`
        window.cookiePunchConfig={value};
    `
}
